{
  "name": "auth-contracts",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/auth-contracts/src",
  "projectType": "library",
  "tags": ["type:contracts", "scope:shared"],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/dist"],
      "options": {
        "command": "tsc",
        "cwd": "packages/auth-contracts"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "eslint . --max-warnings=0",
        "cwd": "packages/auth-contracts"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "echo 'No tests configured for auth-contracts' && exit 0",
        "cwd": "packages/auth-contracts"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit",
        "cwd": "packages/auth-contracts"
      }
    }
  }
}
